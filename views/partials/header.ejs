<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles/styles.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <title>TodoList</title>
  </head>

  
  <body>
    <section id="header">
      <!-- <div class="container">
        
      </div> -->
      <header class="d-flex justify-content-left py-3">
        <div class="container d-flex justify-content-around align-items-center border py-3">
          <div class="dropdown">
            <button class="btn dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
              Show Lists
            </button>
            <ul class="dropdown-menu">
              <form action="/" method="post" class="d-flex">
                <input type="text" name="newList" placeholder="Add new list">
                <input type="submit" value="+">
              </form>
              <li> <a class="dropdown-item" href="/">Today</a></li>
              <% lists.forEach((list) =>{ %>
                 <li> <a class="dropdown-item" href="/<%= list.name %>"><%= list.name %></a></li>
              <% }) %>
            </ul>
        </div>
      </header>
    </section>  
    <section id="main">       

